<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <servlet>
    <servlet-name>hundredsum</servlet-name>
    <servlet-class>mysite.server.Hundred</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>hundredsum</servlet-name>
    <url-pattern>/hundred/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>tensum</servlet-name>
    <servlet-class>mysite.server.Ten</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>tensum</servlet-name>
    <url-pattern>/ten/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>understandrequestgetter</servlet-name>
    <servlet-class>mysite.server.UnderstandRequestGetter</servlet-class>
  </servlet>
